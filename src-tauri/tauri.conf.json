{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "sonnet-shot",
  "version": "0.1.0",
  "identifier": "com.chao.sonnetshot",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:8301",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build"
  },
  "app": {
    "windows": [
        {
            "fullscreen": false,
            "height": 512,
            "resizable": false,
            "title": "Sonnet Shot",
            "width": 830,
            "center": true,
            "decorations": false
        }
    ],
    "macOSPrivateApi": true,
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
